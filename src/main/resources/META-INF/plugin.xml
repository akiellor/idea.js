<idea-plugin version="2">
    <id>org.intellij.ideajs.runconfiguration</id>
    <name>idea.js</name>
    <version>0.0.1</version>
    <vendor email="akiellor@gmail.com" url="http://github.com/akiellor/idea.js">Andrew Kiellor</vendor>

    <description>A plugin to allow user specified, project specific intellij customisations.</description>

    <change-notes><![CDATA[
       <ul>
         <li>Added support to define run configurations in javascript.<li>
       </ul>
    ]]></change-notes>

    <idea-version since-build="107.105"/>

    <extensions defaultExtensionNs="com.intellij">
        <configurationProducer implementation="org.intellij.ideajs.runconfiguration.ProducerShim"/>
    </extensions>
</idea-plugin>
